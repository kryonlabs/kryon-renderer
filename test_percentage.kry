style "tab_content_overlay" {
    width: 100%
    height: 100%
    position: absolute
    pos_x: 0
    pos_y: 0
    background_color: "#FF0000FF"
}

App {
    window_width: 800
    window_height: 600
    window_title: "Percentage Test"
    
    Container {
        style: "tab_content_overlay"
        
        Text {
            text: "This should fill 100% width and height"
            text_color: "#FFFFFF"
            text_alignment: center
        }
    }
}